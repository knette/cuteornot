 <div class="row">
    <div class="col-md-6 offset-md-3">
    <h1>Edit <%= @dog.name %></h1>
    <%= form_for @dog do |f| %>
        <div class="form-group">
        <label>Name</label>
        <%= f.text_field :name, class: "form-control" %>
        </div>
        <div class="form-group">
            <label for="exampleFormControlTextarea1">Picture URL</label>
            <%= f.text_field :picture, class: "form-control" %>
        </div>
        <%= f.submit "Update Doggo", class: "btn btn-primary" %>
    <% end %>
    </div>
</div>
